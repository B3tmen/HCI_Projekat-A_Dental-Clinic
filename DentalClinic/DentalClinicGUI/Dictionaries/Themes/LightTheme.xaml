<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Source: https://atlassian.design/foundations/color-new/color-palette-new -->
    <SolidColorBrush Color="#FFFFFF" x:Key="WhiteTextColor" />
    <SolidColorBrush Color="#FFFFFF" x:Key="BackgroundColor" />
    <SolidColorBrush Color="#ECF0F1" x:Key="ButtonBackgroundColor"/>
